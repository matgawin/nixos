{
    "active_pane_modifiers": {
        "border_size": 0.5,
        "inactive_opacity": 0.8
    },
    "always_treat_brackets_as_autoclosed": true,
    "agent": {
        "button": true,
        "default_model": {
            "model": "claude-sonnet-4",
            "provider": "copilot_chat"
        },
        "dock": "right",
        "enabled": true,
        "enable_feedback": false
    },
    "auto_update": false,
    "auto_update_extensions": {
        "docker-compose": false,
        "dockerfile": false,
        "html": false
    },
    "autosave": "on_window_change",
    "autoscroll_on_clicks": true,
    "buffer_font_family": "MesloLGS Nerd Font",
    "buffer_font_size": 12,
    "buffer_line_height": "comfortable",
    "bottom_dock_layout": "left_aligned",
    "calls": {
        "mute_on_join": true
    },
    "collaboration_panel": {
        "button": false
    },
    "confirm_quit": true,
    "current_line_highlight": "none",
    "cursor_blink": false,
    "diagnostics": {
        "include_warnings": false,
        "inline": {
            "enabled": true,
            "update_debounce_ms": 1000,
            "padding": 4,
            "min_column": 80,
            "max_severity": null
        }
    },
    "double_click_in_multibuffer": "open",
    "ensure_final_newline_on_save": false,
    "features": {
        "edit_prediction_provider": "zed"
    },
    "file_finder": {
        "modal_max_width": "large"
    },
    "file_types": {
        "JSON": [
            "out"
        ]
    },
    "format_on_save": "off",
    "formatter": "language_server",
    "git": {
        "git_gutter": "tracked_files",
        "gutter_debounce": 1000,
        "inline_blame": {
            "delay_ms": 1000,
            "enabled": true,
            "min_column": 60
        }
    },
    "horizontal_scroll_margin": 10,
    "icon_theme": "Colored Zed Icons Theme Light",
    "indent_guides": {
        "enabled": true
    },
    "journal": {
        "hour_format": "hour24"
    },
    "line_indicator_format": "short",
    "lsp": {
        "nil": {
            "initialization_options": {
                "formatting": {
                    "command": [
                        "alejandra"
                    ]
                }
            }
        }
    },
    "max_tabs": 10,
    "project_panel": {
        "auto_reveal_entries": false,
        "default_width": 220,
        "dock": "left",
        "entry_spacing": "standard",
        "indent_size": 10,
        "show_diagnostics": "errors",
        "auto_fold_dirs": false,
        "hide_root": true
    },
    "relative_line_numbers": true,
    "restore_on_startup": "last_session",
    "seed_search_query_from_cursor": "selection",
    "show_completions_on_input": true,
    "show_edit_predictions": true,
    "show_whitespaces": "boundary",
    "show_wrap_guides": true,
    "preferred_line_length": 120,
    "soft_wrap": "preferred_line_length",
    "tab_size": 4,
    "tabs": {
        "activate_on_close": "neighbour",
        "git_status": true,
        "show_close_button": "hidden",
        "show_diagnostics": "errors"
    },
    "title_bar": {
        "show_onboarding_banner": false
    },
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },
    "terminal": {
        "env": {
            "EDITOR": "zed",
            "NO_TMUX": "1"
        },
        "font_family": "MesloLGS Nerd Font",
        "shell": {
            "program": "zsh"
        }
    },
    "theme": {
        "dark": "Nordfox - opaque",
        "light": "Nordfox - opaque",
        "mode": "dark"
    },
    "ui_font_family": "MesloLGS Nerd Font",
    "ui_font_size": 13,
    "use_system_path_prompts": true,
    "vertical_scroll_margin": 5,
    "vim": {
        "toggle_relative_line_numbers": true,
        "use_smartcase_find": true,
        "use_system_clipboard": "always"
    },
    "vim_mode": true,
    "wrap_guides": [
        80,
        120
    ],
    "when_closing_with_no_tabs": "keep_window_open"
}